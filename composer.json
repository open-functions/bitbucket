{
    "name": "open-functions/bitbucket",
    "description": "The Bitbucket Open Function package integrates Bitbucket with large language models (LLMs) using the Open Functions architecture, enabling streamlined repository management and file operations.",
    "homepage": "https://github.com/open-functions/bitbucket",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Weigang",
            "email": "florian@assistant-engine.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "bitbucket/client": "^4.7",
        "guzzlehttp/guzzle": "^7.8",
        "http-interop/http-factory-guzzle": "^1.2",
        "nyholm/psr7": "^1.8@dev",
        "open-functions/open-functions": "dev-main",
        "symfony/http-client": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "OpenFunctions\\Tools\\Bitbucket\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0@dev",
        "filament/filament": "^3.3"
    }
}
